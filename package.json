{
  "name": "infomap-online",
  "version": "1.0.0",
  "private": true,
  "homepage": "/infomap",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "deploy": "scp -r build/* icelab-mapequation:/var/www/infomap"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@mapequation/infomap": "^1.9.0",
    "@sentry/browser": "^5.28.0",
    "arg": "^4.1.3",
    "core-js": "^3.20.0",
    "file-saver": "^2.0.5",
    "jszip": "^3.7.1",
    "localforage": "^1.9.0",
    "mobx": "^6.3.9",
    "mobx-react": "^7.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.2.4",
    "@matejmazur/react-katex": "^3.1.3",
    "react-scripts": "^5.0.0",
    "semantic-ui-react": "^2.0.4",
    "url-search-params-polyfill": "^8.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "prettier": "2.0.5"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 100,
    "trailingComma": "all",
    "overrides": [
      {
        "files": "*.css",
        "options": {
          "tabWidth": 4
        }
      }
    ]
  }
}
